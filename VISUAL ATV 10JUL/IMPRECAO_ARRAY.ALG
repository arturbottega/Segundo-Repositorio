Algoritmo "imprecao_array"

Var

 nomes: vetor[1..3] de caractere
 i: inteiro

Inicio
 // Inicializando o array com valores
 nomes[1] <- "Bottega"
 nomes[2] <- "Pires"
 nomes[3] <- "Vadilson"

   // Imprimindo os valores do array
   para i de 1 ate 3 faca
     escreval(nomes[i], "")

fimpara



Fimalgoritmo